    .slider {
        --slider-wrapper-width: 100%;
        --slider-wrapper-height: auto;
        --slider-item-height: auto;
        --slider-item-margin: 10px;
        --animation-time: 20s;
        --list-width: calc(5 * 500vw);
    }

    @media (width <= 575.98px){
        .slider {
            --slider-item-width: 200px;
        }
    }

    @media (width > 576px){
        .slider {
            --slider-item-width: 250px;
        }
    }

    @media (width > 768px) {
        .slider {
            --slider-item-width: 290px;
        }
    }

    @media (width > 992px) {
        .slider {
            --slider-item-width: 320px;
        }
    }

    @media (width > 1200px) {
        .slider {
            --slider-item-width: 400px;
        }
    }

    @media (width > 1400px) {
        .slider {
            --slider-item-width: 580px;
        }
    }

    .slider {
        width: var(--slider-wrapper-width);
        height: var(--slider-wrapper-height);
        position: relative;
        margin: 20px auto;
    }

    .slide-track {
        display: flex;
        position: relative;
        width: var(--list-width);
    }

    .slide-track-tech {
        animation: infiniteScroll var(--animation-time) linear forwards infinite;
        -webkit-animation: infiniteScroll var(--animation-time) linear forwards infinite;
        margin-top: 3rem;
    }

    .slide-track-tools {
        margin-top: 5rem;
        animation: infiniteScroll var(--animation-time) linear reverse infinite;
        -webkit-animation: infiniteScroll var(--animation-time) linear reverse infinite;
    }

    .slide {
        width: var(--slider-item-width);
        height: var(--slider-item-height);
        float: left;
    }

    .slide svg{
        max-width: 9rem;
    }

    .slider .slide i {
        font-size: 10rem;
        aspect-ratio: 2/1;
    }

    @media (width >= 575px) {
    
        .slider{
            --slider-transformation: -2500px;
        }
    }

    @media (width >= 768px) {
        .img__w-100{
            width: 80%;
        }
        .slider{
            --slider-transformation: -3200px;
        }
        
    }

    @media (width > 992px) {
        
        .slider{
            --slider-transformation: -4000px;
        }
