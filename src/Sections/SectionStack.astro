        }
    }


    @media (width > 1400px) {
        .slider{
            --slider-transformation: -5800px;
        }
    }

    @keyframes infiniteScroll {
        0% {
            left: 0;
        }
        100% {
            left: var(--slider-transformation);
        }
    }

    @-webkit-keyframes infiniteScroll {
        0% {
            left: 0;
        }
        100% {
            left: var(--slider-transformation);
        }
    }
